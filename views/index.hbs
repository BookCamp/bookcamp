<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>
{{#each posts}}
<div>
    <div>
        {{this.creator.username}}
        {{this.school.name}}
        {{this.course}}
    </div>
    {{this.title}}

    {{#if this.url}}
    {{this.url}}
    {{/if}}

    {{#if this.comment}}
    {{this.comment}}
    {{/if}}


    {{#if this.file}}
    {{this.file}}
    {{/if}}

    {{#if this.img}}
    <img src="{{this.img}}" alt="Photo">
    {{/if}}

    {{#if this.location}}
    {{this.location}}
    {{/if}}
</div>
{{/each}}